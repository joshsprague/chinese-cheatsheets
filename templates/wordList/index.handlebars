<div class="list">
  <h1>Chinese Cheatsheets</h1>
  <h3>Enter English words to build a new list</h3>
  <table>
    {{#collection tag="ul"}}
      <tr>
        <td class="keyword">{{title}}</td><td>{{translation}}</td>
      </tr>
    {{/collection}}
    <form>
      <input name="title">
      <input type="submit" value="Translate">
    </form>
  </table>
  <div class="email">
    <button name="save" class="save">Email this list</button></p>
  </div>
</div>

